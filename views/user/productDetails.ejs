	<!-- ============ADD TO CART FETCH REQ================ -->

	<script>
		let onCartBtnClick = function() {
			console.log("btn clicked");
			const cartbtn = document.getElementById("addToCart_btn");
			const productId = cartbtn.getAttribute('data-product-id');
			
			console.log(productId);
	
			fetch(`/productaddtocart/${productId}`)
				.then(response => response.json())
				.then(data => {
					if (data) {
						// If you want to hide the button upon success
						// cartbtn.style.display = "none";
						// Or redirect to cart
						// window.location.href = "/cart"; 
					} else {
						window.location.reload(); // Reload the page if there's an error
					}
				})
				.catch(error => console.error('Error:', error));
		}
	</script>
